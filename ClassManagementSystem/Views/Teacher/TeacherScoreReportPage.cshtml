@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head lang="en">
    <meta charset="UTF-8">
    <title>教师查看学生报告</title>
    <link rel="stylesheet" href="{$base}/raffle/bootstrap.min.css">
    <style>
        html {
            height: 100%;
            margin: 0;
        }

        body {
            height: 100%;
            margin: 0;
        }

        .top {
            width: 90%;
            height: 8%;
            margin-top: 3%;
        }

        .body {
            width: 90%;
            height: 92%;
            margin-left: 5%;
        }

        .file {
            width: 100%;
            height: 100%;
            border-style: solid;
            border-width: %1;
            border-color: black;
        }

        .pdfFile {
            font-family: YouYuan;
            padding-left: 5%;
        }

        .item1 {
            width: 40%;
            height: 5%;
            margin-left: 5%;
        }

        .item2 {
            width: 40%;
            height: 5%;
            margin-left: 85%;
        }

        .itemName {
            font-family: YouYuan;
            font-size: 20px;
            padding-left: 3%;
        }
    </style>
</head>
<body>

    <div class="top">
        <div class="item1">
            <label class="itemName" id="seminarName">讨论课3</label>
            <label class="itemName" id="groupName">1-A-1组</label>
            <label class="itemName" id="leader">组长：……</label>
        </div>
        <div class="item2">
            <label class="itemName">评分:</label>
            <input type="text" placeholder="3-5" id="inputScore" />
            <button class="itemName">确认打分</button>
        </div>
    </div>
    @*<div class="body">
        <div class="file">
            <object class="pdfFile" data="/Student/233.pdf" type="application/pdf">学生的报告</object>
        </div>
    </div>*@
    <div class="body"> @*???*@
        <object class="file" data="~/Student/233.pdf" type="application/pdf"></object>
    </div>
    @*<div class="body">
        <object class="file" data="./233.pdf" type="application/pdf"></object>
    </div>
    <!-- Tell PDFObject which PDF to embed, and where to embed it -->
    <!-- <script src="/js/pdfobject.js"></script>
    <script>PDFObject.embed("/pdf/sample-3pp.pdf", "#example1");</script> -->*@
    <script src="//code.jquery.com/jquery-1.10.2.js"></script>
    <script>
        <div class="top">
            <div class="item1">
                <label class="itemName" id="seminarName">讨论课3</label>
                <label class="itemName" id="groupName">1-A-1组</label>
                <label class="itemName" id="leader">组长：……</label>
            </div>
            <div class="item2">
                <label class="itemName">评分:</label>
                <input type="text" placeholder="3-5" id="inputScore" />
                <button class="itemName">确认打分</button>
            </div>
        </div>
        $(document).ready(function () {
            var groupId =;  //获得上一个页面传来的小组ID
            $.ajax({
                type: "GET",
                url: "/group/{groupId}",
                contentType: "application/json;charset=utf-8",
                dataType: "json",
                success: function (data, status) {

                    $("#pic").attr("src", data.avatar);
                    $("#userName").text(data.id);
                    $("#studentId").text(data.number);
                    $("#studentName").text(data.name);
                    if (data.gender == "male")
                        $("#studentGender").text("男");
                    else
                        $("#studentGender").text("女");
                    $("#studentSchool").text(data.school.name);
                    $("#studentEducation").text(data.title);
                    $("#studentEmail").text(data.email);
                    $("#studentPhone").text(data.phone);
                }
            });
        });
    </script>
</body>
</html>